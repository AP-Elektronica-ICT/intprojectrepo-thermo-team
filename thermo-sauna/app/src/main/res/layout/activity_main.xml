<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity"
    android:background="@drawable/background">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="661dp"
        android:orientation="vertical"
        android:paddingLeft="16dp"
        android:paddingRight="16dp">


        <com.jjoe64.graphview.GraphView

            android:id="@+id/grafiek"
            android:layout_width="380dp"
            android:layout_height="308dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="69dp"
            android:layout_weight="1"
            android:gravity="top"

            android:hint="" />

        <ImageView
            android:id="@+id/vuur"
            android:layout_width="71dp"
            android:layout_height="92dp"
            android:layout_above="@+id/view3"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="11dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="30dp"
            android:paddingTop="0dp"
            app:srcCompat="@drawable/vuur" />

        <TextView
            android:id="@+id/locatie"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/view3"
            android:layout_alignParentTop="true"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="32dp"
            android:layout_toEndOf="@+id/vuur"
            android:text="TextView" />

        <TextView
            android:id="@+id/tijd"
            android:layout_width="113dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="false"
            android:layout_alignParentTop="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="174dp"
            android:text="Duration:" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/grafiek"
            android:layout_alignParentStart="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="-50dp"
            android:text="TextView4" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/grafiek"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="-50dp"
            android:layout_marginEnd="80dp"
            android:text="Session AVG" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView9"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:text="Temperature AVG" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView9"
            android:layout_marginStart="14dp"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@+id/textView9"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="9dp"
            android:layout_marginBottom="7dp"
            android:layout_toEndOf="@+id/textView11"
            android:text="TextView" />

        <TextView
            android:id="@+id/durationdata"
            android:layout_width="114dp"
            android:layout_height="50dp"
            android:layout_above="@+id/view2"
            android:layout_below="@+id/tijd"
            android:layout_alignParentStart="false"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="10dp" />

        <TextView
            android:id="@+id/tempdata"
            android:layout_width="149dp"
            android:layout_height="34dp"
            android:layout_above="@+id/view2"
            android:layout_below="@+id/temp"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="161dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="5dp"
            android:layout_toEndOf="@+id/durationdata"
            android:text="20" />

        <TextView
            android:id="@+id/temp"
            android:layout_width="141dp"
            android:layout_height="20dp"
            android:layout_alignTop="@+id/vuur"
            android:layout_marginStart="159dp"
            android:layout_marginTop="170dp"
            android:layout_toEndOf="@+id/tijd"
            android:text="Temperature:" />


        <TextView
            android:id="@+id/textView12"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textView10"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="123dp"
            android:layout_marginTop="-20dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="5dp"
            android:layout_toEndOf="@+id/textView5"
            android:text="TextView" />

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_above="@+id/grafiek"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="2dp"
            android:layout_marginBottom="34dp"
            android:background="@color/White" />

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_above="@+id/tijd"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="2dp"
            android:layout_marginBottom="-164dp"
            android:background="@color/White" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/view3"
            android:layout_alignParentTop="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="33dp"
            android:layout_toEndOf="@+id/locatie"
            android:text="Status:" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="67dp"
            android:layout_height="113dp"
            android:layout_alignBottom="@+id/view3"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="34dp"
            android:layout_toEndOf="@+id/textView"
            android:text="aan" />


    </RelativeLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dp"
        app:itemBackground="@color/White"
        app:itemIconTint="@drawable/selector"
        app:itemTextColor="@drawable/selector"
        app:menu="@menu/menu_navigation" />


</RelativeLayout>




